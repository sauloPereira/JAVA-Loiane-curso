package com.loiane.java.aula46labs;

public class Circulo extends Figura2D implements DimensaoSuperficial{
	
	private double raio;

	public double getRaio() {
		return raio;
	}

	public void setRaio(double raio) {
		this.raio = raio;
	}

	@Override
	public double calcularArea() {
		double a = Math.PI;
		
		return a;
	}

}
